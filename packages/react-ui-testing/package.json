{
  "name": "react-ui-testing",
  "private": true,
  "version": "3.1.3",
  "description": "React selenium testing",
  "main": "react-selenium-testing.js",
  "scripts": {
    "postinstall": "yarn --cwd ./TestPages install",
    "build": "dotnet build --configuration Release",
    "test": "dotnet test ./Tests/Tests.csproj --configuration Release --verbosity detailed --framework netcoreapp3.1",
    "start": "yarn install-chrome && run-p start:*",
    "start:pages": "yarn --cwd ./TestPages start",
    "start:chrome": "chromedriver --port=4444",
    "install-chrome": "npm install chromedriver --no-save"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "npm-run-all": "4.1.5"
  }
}
